{
  "name": "openpublicmedia/engaging-networks-php",
  "description": "PHP client for Engaging Networks Services (ENS) APIs",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.0 || ^7.0"
  },
  "require-dev": {
    "phpcompatibility/php-compatibility": "^9.3",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.4",
    "tochka-developers/array-file-cache": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "OpenPublicMedia\\EngagingNetworksServices\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenPublicMedia\\EngagingNetworksServices\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
    "scripts": {
        "lint": [
            "php vendor/bin/phpcs",
            "php vendor/bin/phpstan --memory-limit=-1"
        ],
        "test": "php vendor/bin/phpunit"
    }
}
